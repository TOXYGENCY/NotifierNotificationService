{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseSwagger": true,
  "RabbitMq": {
    "HostName": "rabbitmq",
    "UserName": "admin",
    "Password": "admin",
    "NotificationsQueueName": "notifications",
    "StatusUpdatesQueueName": "status_updates"
  },
  "Serilog": {
    "MinimumLevel": {
      "File": "Information",
      "Console": "Information"
    }
  },
    "Redis": {
      "ConnectionString": "redis:6379",
      "InstanceName": "notifier",
      "Statuses": {
        "CategoryName": "statuses"
      },
      "Streams": {
        "Statuses": "status_log"
      }
    }
  }
